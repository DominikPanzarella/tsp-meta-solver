{
    "version": 3,
    "cmakeMinimumRequired": {
      "major": 3,
      "minor": 21,
      "patch": 0
    },
    "configurePresets": [
      {
        "name": "debug",
        "displayName": "Debug with Qt GUI",
        "generator": "Ninja",
        "binaryDir": "${sourceDir}/build-debug",
        "cacheVariables": {
          "CMAKE_BUILD_TYPE": "Debug",
          "CMAKE_PREFIX_PATH": "/Users/dominikpanzarella/Qt/6.9.1/macos/lib/cmake",
          "ENABLE_QT": "ON"
        }
      },
      {
        "name": "release",
        "displayName": "Release with Qt GUI",
        "generator": "Ninja",
        "binaryDir": "${sourceDir}/build-release",
        "cacheVariables": {
          "CMAKE_BUILD_TYPE": "Release",
          "CMAKE_PREFIX_PATH": "/Users/dominikpanzarella/Qt/6.9.1/macos/lib/cmake",
          "ENABLE_QT": "ON"
        }
      },
      {
        "name": "cli-backend-only",
        "displayName": "Release CLI Backend Only",
